#Description:

SIMULATION
  SIMULATION_TYPE SUBSURFACE
  PROCESS_MODELS
    SUBSURFACE_FLOW flow
      MODE GENERAL
      OPTIONS
        #  WINDOW_EPSILON 1.d-4
        ITOL_SCALED_RESIDUAL 1.d-5
        ISOTHERMAL
        TWO_PHASE_ENERGY_DOF TEMPERATURE
        ARITHMETIC_GAS_DIFFUSIVE_DENSITY
      /
    /
  /
END

SUBSURFACE

#=========================== flow mode ========================================

REGRESSION
  CELLS_PER_PROCESS 5
  CELLS
    89
    111
    113
    133
  /
END

#=========================== debug settings ===================================

skip
USE_TOUCH_OPTIONS
DEBUG
  MATVIEW_JACOBIAN
  VECVIEW_RESIDUAL
#  VECVIEW_SOLUTION
/
noskip

#=========================== time stepper options =============================
TIMESTEPPER FLOW
  TS_ACCELERATION 8
  MAX_TS_CUTS 10
/

#=========================== discretization ===================================
GRID
  TYPE structured
  NXYZ 11 1 21
  DXYZ
    0.5d0 10*1.d0
    1.d0
    21*1.d0
  /
/

#=========================== solver options ===================================
TIMESTEPPER FLOW
#  DT_FACTOR 2.d0 1.8d0 1.6d0 1.4d0 1.2d0 1.d0 0.8d0 0.6d0 0.4d0
END

NEWTON_SOLVER FLOW
#  ATOL 1.d-30
#  RTOL 1.d-30
  ATOL 1.d-8
  RTOL 1.d-8
  STOL 1.d-30
  NO_INFINITY_NORM
  MAXIT 15
/

LINEAR_SOLVER FLOW
  SOLVER DIRECT
/

#=========================== fluid properties =================================
FLUID_PROPERTY
  DIFFUSION_COEFFICIENT 1.d-9
/

EOS WATER
  DENSITY EXPONENTIAL 996.629 101325.d0 4.45d-10
  VISCOSITY CONSTANT 8.5077d-4
  ENTHALPY CONSTANT 1.8890d6
/

#=========================== material properties ==============================
MATERIAL_PROPERTY soil
  ID 1
  CHARACTERISTIC_CURVES cc1
  POROSITY 0.45
  TORTUOSITY 1.
  ROCK_DENSITY 2650.d0
  THERMAL_CONDUCTIVITY_DRY 0.5
  THERMAL_CONDUCTIVITY_WET 2.
  HEAT_CAPACITY 830.
  SOIL_COMPRESSIBILITY_FUNCTION BRAGFLO
  BULK_COMPRESSIBILITY 1.d-8
  SOIL_REFERENCE_PRESSURE 101325.d0
  PERMEABILITY
    PERM_ISO 1.d-17
  /
/

#=========================== saturation functions =============================
CHARACTERISTIC_CURVES cc1
  SATURATION_FUNCTION VAN_GENUCHTEN
    LIQUID_RESIDUAL_SATURATION 0.2d0
    M 0.7d0
    ALPHA 9.869d-6
    MAX_CAPILLARY_PRESSURE 1.d8
  /
  PERMEABILITY_FUNCTION MUALEM
    PHASE LIQUID
    LIQUID_RESIDUAL_SATURATION 0.2d0
    M 0.7d0
  /
  PERMEABILITY_FUNCTION MUALEM_VG_GAS
    PHASE GAS
    LIQUID_RESIDUAL_SATURATION 0.2d0
    GAS_RESIDUAL_SATURATION 1.d-5
    M 0.7d0
  /
/

#=========================== output options ===================================
OUTPUT
#  PERIODIC_OBSERVATION TIMESTEP 1
#  TIMES s 1.d2 1.d3 1.d4 1.d5 1.d6
#  VELOCITY_AT_CENTER
#  FORMAT TECPLOT BLOCK
#  FORMAT HDF5
  VARIABLES
    TEMPERATURE
    LIQUID_PRESSURE
    GAS_PRESSURE
    AIR_PRESSURE
    LIQUID_SATURATION
    GAS_SATURATION
    LIQUID_DENSITY
    GAS_DENSITY
    LIQUID_MOLE_FRACTIONS
    GAS_MOLE_FRACTIONS
    LIQUID_ENERGY # PER_VOLUME
    GAS_ENERGY # PER_VOLUME
    THERMODYNAMIC_STATE
    POROSITY
  /
/

#=========================== times ============================================
TIME
  FINAL_TIME 1.d7 s ! 0.317... y
  INITIAL_TIMESTEP_SIZE 1.d0 s
  MAXIMUM_TIMESTEP_SIZE 100.d0 s
  MAXIMUM_TIMESTEP_SIZE 1.d5 y at 5000 s
/

#=========================== regions ==========================================
REGION all
  COORDINATES
    0.d0 0.d0 0.d0
    10.5d0 1.d0 21.d0
  /
/

REGION top
  FACE top
  COORDINATES
    0.d0 0.d0 21.d0
    10.5d0 1.d0 21.d0
  /
/

REGION injection_point
  COORDINATE 0.d0 0.5d0 10.5d0
/

REGION 1
  BLOCK 1 11 1 1 1 1
END

