#Description: 3D test problem for variably saturated flow and tracer transport

SIMULATION
  SIMULATION_TYPE SUBSURFACE
  PROCESS_MODELS
    SUBSURFACE_FLOW flow
      MODE RICHARDS
    /
    SUBSURFACE_TRANSPORT transport
      GLOBAL_IMPLICIT
    /
  /
END

SUBSURFACE

#=========================== regression =======================================
REGRESSION
  CELLS
    ! eight corners
    1
    5
    16
    20
    41
    45
    56
    60
    ! center of each side
    8 ! west
    23 ! south
    26 ! west
    35 ! east
    38 ! north
    54 ! top
  /
END

#=========================== flow mode ========================================

DEBUG
#  PRINT_COUPLERS west south
END

DATASET surface
  MAX_BUFFER_SIZE 5
  FILENAME 543_datum_dataset_in.h5
  HDF5_DATASET_NAME test_surface
END

#=========================== chemistry ========================================
CHEMISTRY
  PRIMARY_SPECIES
    Tracer
  /
  OUTPUT
    TOTAl
  /
/

#=========================== solver options ===================================
TIMESTEPPER FLOW
  TS_ACCELERATION 8
/

NEWTON_SOLVER FLOW
/

LINEAR_SOLVER FLOW
  KSP_TYPE PREONLY
  PC_TYPE LU
/

NEWTON_SOLVER TRANSPORT
/

LINEAR_SOLVER TRANSPORT
  KSP_TYPE PREONLY
  PC_TYPE LU
/

#=========================== discretization ===================================
GRID
  TYPE structured
  ORIGIN 0.d0 0.d0 0.d0
  NXYZ 5 4 3
  DXYZ
    10. 11. 12. 13. 14.
    13. 12. 11. 10.
    15. 20. 25.
  /
END

#=========================== fluid properties =================================
FLUID_PROPERTY
  DIFFUSION_COEFFICIENT 1.d-9
/

#=========================== material properties ==============================
MATERIAL_PROPERTY soil1
  ID 1
  POROSITY 0.25d0
  TORTUOSITY 1.d0
  CHARACTERISTIC_CURVES sf1
  PERMEABILITY
    PERM_X 1.d-12
    PERM_Y 1.d-12
    PERM_Z 1.d-13
  /
/

MATERIAL_PROPERTY soil2
  ID 2
  POROSITY 0.25d0
  TORTUOSITY 1.d0
  CHARACTERISTIC_CURVES sf2
  PERMEABILITY
    PERM_X 1.d-11
    PERM_Y 1.d-11
    PERM_Z 1.d-12
  /
/

MATERIAL_PROPERTY soil3
  ID 3
  POROSITY 0.2d0
  TORTUOSITY 1.d0
  CHARACTERISTIC_CURVES sf3
  PERMEABILITY
    PERM_X 1.d-10
    PERM_Y 1.d-10
    PERM_Z 1.d-11
  /
/

MATERIAL_PROPERTY soil4
  ID 4
  POROSITY 0.25d0
  TORTUOSITY 1.d0
  CHARACTERISTIC_CURVES sf4
  PERMEABILITY
    PERM_X 1.d-9
    PERM_Y 1.d-9
    PERM_Z 1.d-10
  /
/

#=========================== characteristic curves ============================
CHARACTERISTIC_CURVES sf1
  SATURATION_FUNCTION VAN_GENUCHTEN
    M 0.286
    ALPHA  1.9401d-4
    LIQUID_RESIDUAL_SATURATION 0.115
  /
  PERMEABILITY_FUNCTION MUALEM_VG_LIQ
    M 0.286
    LIQUID_RESIDUAL_SATURATION 0.115
  /
END

CHARACTERISTIC_CURVES sf2
  SATURATION_FUNCTION VAN_GENUCHTEN
    M 0.486
    ALPHA  3.8801d-4
    LIQUID_RESIDUAL_SATURATION 0.110
  /
  PERMEABILITY_FUNCTION MUALEM_VG_LIQ
    M 0.486
    LIQUID_RESIDUAL_SATURATION 0.110
  /
END

CHARACTERISTIC_CURVES sf3
  SATURATION_FUNCTION VAN_GENUCHTEN
    M 0.541
    ALPHA 1.0211d-4
    LIQUID_RESIDUAL_SATURATION 0.118
  /
  PERMEABILITY_FUNCTION MUALEM_VG_LIQ
    M 0.541
    LIQUID_RESIDUAL_SATURATION 0.118
  /
END

CHARACTERISTIC_CURVES sf4
  SATURATION_FUNCTION VAN_GENUCHTEN
    M 0.527
    ALPHA 5.1054d-5
    LIQUID_RESIDUAL_SATURATION 0.143
  /
  PERMEABILITY_FUNCTION MUALEM_VG_LIQ
    M 0.527
    LIQUID_RESIDUAL_SATURATION 0.143
  /
END

#=========================== output options ===================================
OUTPUT
  TIMES d 0.001 0.01 0.1 1. 10. 30. 50. 75.
!  FORMAT TECPLOT BLOCK
!  FORMAT TECPLOT POINT
!  FORMAT HDF5
!  VELOCITY_AT_CENTER
/

#=========================== times ============================================
TIME
  FINAL_TIME 100.d0 d
  INITIAL_TIMESTEP_SIZE 1.d-6 d
  MAXIMUM_TIMESTEP_SIZE 10.d0 d
/

#=========================== regions ==========================================
REGION all
  COORDINATES
    0.d0 0.d0 0.d0
    60.d0 46.d0 60.d0
  /
END

REGION top
  FACE TOP
  COORDINATES
    0.d0 0.d0 60.d0
    60.d0 46.d0 60.d0
  /
END

REGION bottom
  FACE BOTTOM
  COORDINATES
    0.d0 0.d0 0.d0
    60.d0 46.d0 0.d0
  /
END

REGION west
  FACE WEST
  COORDINATES
    0.d0 0.d0 0.d0
    0.d0 46.d0 60.d0
  /
END

REGION east
  FACE EAST
  COORDINATES
    60.d0 0.d0 0.d0
    60.d0 46.d0 60.d0
  /
END

REGION north
  FACE NORTH
  COORDINATES
    0.d0 46.d0 0.d0
    60.d0 46.d0 60.d0
  /
END

REGION south
  FACE SOUTH
  COORDINATES
    0.d0 0.d0 0.d0
    60.d0 0.d0 60.d0
  /
END

REGION well
  BLOCK 4 4 2 3 3 3
END

#=========================== flow conditions ==================================
FLOW_CONDITION initial
  TYPE
    PRESSURE hydrostatic
  /
  DATUM 0.d0 0.d0 35.d0
  PRESSURE 101325 ! Pa
/

FLOW_CONDITION east
  TYPE
    PRESSURE hydrostatic
  /
#  DATUM 0.d0 0.d0 25.d0
  DATUM DATASET surface
  PRESSURE 101325 ! Pa
/

FLOW_CONDITION west_condition
  TYPE
    PRESSURE hydrostatic
  /
#  GRADIENT
#    PRESSURE file gradient.txt
#    PRESSURE file gradient_x.txt
#  /
#  DATUM file datum.txt
  DATUM DATASET surface
  PRESSURE 101325 ! Pa
/

FLOW_CONDITION south_condition
  TYPE
    PRESSURE hydrostatic
  /
#  GRADIENT
#    PRESSURE file gradient.txt
#    PRESSURE file gradient_y.txt
#  /
#  DATUM file datum.txt
  DATUM DATASET surface
  PRESSURE 101325 ! Pa
/

FLOW_CONDITION injection_well
  TYPE
    RATE volumetric_rate
  /
  RATE 2.77777778d-4  ! 1 m^3/hr
/

#=========================== transport conditions =============================
TRANSPORT_CONDITION initial
  TYPE dirichlet_zero_gradient
  CONSTRAINT_LIST
    0.d0 initial
  /
END

TRANSPORT_CONDITION well_concentration
  TYPE dirichlet
  CONSTRAINT_LIST
    0.d0 source
  /
/

#=========================== constraints ======================================
CONSTRAINT source
  CONCENTRATIONS
    Tracer 1.d0 T
  /
END

CONSTRAINT initial
  CONCENTRATIONS
    Tracer 1.d-40 T
  /
END

#=========================== condition couplers ===============================
# initial condition
INITIAL_CONDITION
  FLOW_CONDITION initial
  TRANSPORT_CONDITION initial
  REGION all
END

# west boundary condition
BOUNDARY_CONDITION west
  FLOW_CONDITION west_condition
  TRANSPORT_CONDITION initial
  REGION west
END

skip
# east boundary condition
BOUNDARY_CONDITION east
  FLOW_CONDITION initial
  TRANSPORT_CONDITION initial
  REGION east
END

# north boundary condition
BOUNDARY_CONDITION north
  FLOW_CONDITION initial
  TRANSPORT_CONDITION initial
  REGION north
END
noskip

# south boundary condition
BOUNDARY_CONDITION south
  FLOW_CONDITION  south_condition
  TRANSPORT_CONDITION initial
  REGION south
END

# well source/sink
SOURCE_SINK well
  FLOW_CONDITION injection_well
  TRANSPORT_CONDITION well_concentration
  REGION well
END

#=========================== stratigraphy couplers ============================
STRATA
  MATERIAL ./543.h5
END

END_SUBSURFACE
