
SIMULATION
  SIMULATION_TYPE SUBSURFACE
  PROCESS_MODELS
    SUBSURFACE_FLOW flow
      MODE MPHASE
    /
  /
END

SUBSURFACE


CO2_DATABASE ../../../../database/co2data0.dat

#=========================== regression test ========================================
REGRESSION
  CELLS
    2086
  /
  CELLS_PER_PROCESS 2
END

#=========================== discretization ===================================
GRID
TYPE structured
ORIGIN 0.d0 0.d0 0.d0
NXYZ 107  1  51
BOUNDS
0.d0 0.d0 0.d0
321.d0 1.d0 51.d0
/
END
#
#=========================== solver options ===================================
TIMESTEPPER FLOW
TS_ACCELERATION 8
/

NEWTON_SOLVER FLOW
ATOL 1D-12
RTOL 1D-12
STOL 1D-30
DTOL 1D15
ITOL 1D-8
MAXIT 25
MAXF 100
END
#

#=========================== times ============================================
TIME
FINAL_TIME 0.25d0 y
INITIAL_TIMESTEP_SIZE 1.d-6 y
MAXIMUM_TIMESTEP_SIZE 50.d0 y
#MAXIMUM_TIMESTEP_SIZE 50.d0 y at 15. y
MAXIMUM_TIMESTEP_SIZE 200.d0 y at 50. y
MAXIMUM_TIMESTEP_SIZE 500.d0 y at 20000. y
MAXIMUM_TIMESTEP_SIZE 1000.d0 y at 50000. y
MAXIMUM_TIMESTEP_SIZE 5000.d0 y at 100000. y
/

#=========================== output options ===================================
skip
OUTPUT
MASS_BALANCE
PRINT_COLUMN_IDS
PERIODIC_OBSERVATION TIMESTEP 1
#TIMES y 0.01 0.1 0.25 0.5 0.75 1.0 2.5 5. 7.5 10. \
#25. 50. 75. 100. 250. 500. 750. 1000. 2500. 5000. \
#7500. 1.e4 12500. 15000. 17500. 20000. 25000. 30000. 35000. 40000. \
#55000. 50000. 60000. 70000. 80000. 90000. 100000. 250000. 500000. \
#750000. 1000000.
FORMAT TECPLOT POINT
FORMAT HDF5
VELOCITY_AT_CENTER
/
noskip

#=========================== fluid properties =================================
FLUID_PROPERTY
LIQUID_DIFFUSION_COEFFICIENT 1.d-9
GAS_DIFFUSION_COEFFICIENT 2.13d-5
/

#=========================== material properties ==============================
MATERIAL_PROPERTY soil1
ID 1
POROSITY 0.15d0
TORTUOSITY 1d-1
ROCK_DENSITY 2.65E3
SPECIFIC_HEAT 1E3
THERMAL_CONDUCTIVITY_DRY 0.5
THERMAL_CONDUCTIVITY_WET 0.5
SATURATION_FUNCTION sf2
PERMEABILITY
PERM_X 1.d-15
PERM_Y 1.d-15
PERM_Z 1.d-17
/
/
#=========================== saturation functions =============================
SATURATION_FUNCTION sf2
PERMEABILITY_FUNCTION_TYPE NMT_EXP
SATURATION_FUNCTION_TYPE NMT_EXP
RESIDUAL_SATURATION LIQUID_PHASE 0.1
RESIDUAL_SATURATION GAS_PHASE 0.0
LAMBDA 0.762d0
ALPHA 7.5d-4
MAX_CAPILLARY_PRESSURE 1.d6
BETAC 2.d0
POWER 7.d0
/

#=========================== regions ==========================================
REGION all
COORDINATES
0.d0 0.d0 0.d0
321.D0 1.D0  51.D0
/
END

REGION top
FACE TOP
COORDINATES
0.d0 0.d0  51.d0
321.d0 1.d0 51.d0
/
END

REGION west
FACE WEST
COORDINATES
0.d0 0.d0 0.d0
0.d0 1.d0 51.d0
/
END

REGION east
FACE EAST
COORDINATES
321.d0 0.d0 0.d0
321.d0 1.d0 51.d0
/
END

REGION well
COORDINATES
160.d0 1.d0 20.d0
160.d0 1.d0 20.d0
/
END

#=========================== flow conditions ==================================
FLOW_CONDITION initial
UNITS Pa,C,M,yr
TYPE
PRESSURE hydrostatic
TEMPERATURE zero_gradient
CONCENTRATION zero_gradient
ENTHALPY dirichlet
/
IPHASE 1
PRESSURE 2.D7 2.D7
TEMPERATURE 50.d0 C
CONCENTRATION 1.d-6 ! iphase = 1,2: mole fraction CO2; iphase = 3: saturation
ENTHALPY 0.d0 0.d0
/

FLOW_CONDITION top
UNITS Pa,C,M,yr
TYPE
PRESSURE dirichlet
TEMPERATURE zero_gradient
CONCENTRATION zero_gradient
ENTHALPY dirichlet
/
IPHASE 1
PRESSURE 2.D7 2.D7
TEMPERATURE 50.d0
CONCENTRATION 1.d-6
ENTHALPY 0.d0 0.d0
/

FLOW_CONDITION source
UNITS Pa,C,M,yr
SYNC_TIMESTEP_WITH_UPDATE
TYPE
RATE mass_rate
PRESSURE dirichlet
TEMPERATURE dirichlet
CONCENTRATION dirichlet
ENTHALPY dirichlet
/
RATE LIST
  TIME_UNITS y
  DATA_UNITS kg/s
0.  0. 1.d-4
10. 0. 0.
/
PRESSURE 2.d7 2.d7
TEMPERATURE 50.d0
CONCENTRATION 0.D0
ENTHALPY 0.d0 0.d0
/

#=========================== condition couplers ===============================
# initial condition
INITIAL_CONDITION
FLOW_CONDITION initial
REGION all
END

BOUNDARY_CONDITION west
FLOW_CONDITION initial
REGION west
END

BOUNDARY_CONDITION east
FLOW_CONDITION initial
REGION east
END

SOURCE_SINK
FLOW_CONDITION source
REGION well
END
#
#=========================== stratigraphy couplers ============================
STRATA
REGION all
MATERIAL soil1
END
END_SUBSURFACE
