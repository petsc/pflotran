!=========================== solver options ===================================
TIMESTEPPER FLOW
TS_ACCELERATION 16
!MAX_STEPS 10
/


TIMESTEPPER TRANSPORT
TS_ACCELERATION 16
!MAX_STEPS 10
/


!NEWTON_SOLVER FLOW
!  ATOL 1D-8
!  RTOL 1D-8
!  STOL 1D-30
!  DTOL 1D15
!  ITOL 1D-8
!  MAXIT 25
!  MAXF 100
!END


NEWTON_SOLVER FLOW
ATOL 1D-8
RTOL 1D-6
STOL 1D-30
DTOL 1D4
ITOL 1D-8
!MAXIT 25
!MAXF 100
!PRINT_DETAILED_CONVERGENCE
END



NEWTON_SOLVER TRANSPORT
ATOL 1D-8
RTOL 1D-6
STOL 1D-30
DTOL 1D4
ITOL 1D-8
!MAXIT 25
!MAXF 100
!PRINT_DETAILED_CONVERGENCE
END

!=========================== discretization ===================================
GRID
TYPE structured
  ORIGIN 0.d0 0.d0 0.d0
  NXYZ 4 4 5
  BOUNDS
    0.d0 0.d0 0.d0
    40.d0 40.d0 10.d0
  /
END

!=========================== times ============================================
TIME
FINAL_TIME 3.5d-6 y
INITIAL_TIMESTEP_SIZE 1.d-6 y
MAXIMUM_TIMESTEP_SIZE 1.d-2 y
MAXIMUM_TIMESTEP_SIZE 1.d0 y at 10. y
/

!=========================== fluid properties =================================
FLUID_PROPERTY 
  LIQUID_DIFFUSION_COEFFICIENT 1.d-9
  GAS_DIFFUSION_COEFFICIENT 2.13d-5
END

!=========================== output options ===================================
skip
OUTPUT
  MASS_BALANCE DETAILED
  TIMES y 1e-3 1e-2 1 10. 20. 40. 100. 250. 500. 750. 1000. 1250. 1500. 1750.  2000.
  PERIODIC_OBSERVATION TIMESTEP 1
!  FORMAT TECPLOT BLOCK
  FORMAT HDF5 MULTIPLE_FILES
  PRINT_COLUMN_IDS
!  VELOCITIES
!  PERMEABILITY
END
noskip

