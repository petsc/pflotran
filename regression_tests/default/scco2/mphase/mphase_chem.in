  POROSITY 0.1d0
  TORTUOSITY 1.d0
  ROCK_DENSITY 2.65d3
  SPECIFIC_HEAT 1.d3
  THERMAL_CONDUCTIVITY_DRY 2.5
  THERMAL_CONDUCTIVITY_WET 2.5 
  SATURATION_FUNCTION sf1
  PERMEABILITY
  ! ANISOTROPIC
